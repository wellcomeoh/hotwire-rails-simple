<p id="notice"><%= notice %></p>

<h1>Jobbs</h1>

<%= turbo_frame_tag 'jobbs' do %>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @jobbs.each do |jobb| %>
        <tr>
          <td><%= jobb.title %></td>
          <td><%= jobb.description %></td>
          <td><%= link_to 'Show', jobb %></td>
          <td><%= link_to 'Edit', edit_jobb_path(jobb) %></td>
          <td><%= button_to 'Destroy', jobb, method: :delete %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>

<br>

<%= link_to 'New Jobb', new_jobb_path %>
